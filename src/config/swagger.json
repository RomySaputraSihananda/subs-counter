{
  "swagger": "2.0",
  "info": {
    "title": "Sub-Counter-Service",
    "description": "Rest API to show detail counter of subscribers and views from channel youtube",
    "version": "v1.0.0",
    "license": {
      "name": "Licensed Under MIT",
      "url": "https://spdx.org/licenses/MIT.html"
    }
  },
  "produces": ["application/json"],
  "consumes": ["application/json"],
  "paths": {
    "/api/v1/search/{name}": {
      "get": {
        "x-swagger-router-controller": "check",
        "operationId": "index",
        "tags": ["Channel-Controller"],
        "description": "API to search channel youtube",
        "parameters": [
          {
            "in": "path",
            "name": "name",
            "description": "username of youtube channel",
            "required": true,
            "schema": {
              "type": "string",
              "example": "windah basudara"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "200 response",
            "examples": {
              "application/json": {
                "status": "string",
                "code": 200,
                "message": "string",
                "data": [
                  {
                    "id": "string",
                    "title": "string",
                    "avatar": "string",
                    "banner": "string",
                    "isBanner": "bool",
                    "subscriberCount": "string",
                    "hiddenSubscriberCount": "bool"
                  }
                ]
              }
            }
          }
        }
      }
    },
    "/api/v1/channel/{id}": {
      "get": {
        "x-swagger-router-controller": "check",
        "operationId": "index",
        "tags": ["Bank-Controller"],
        "description": "API to get detail subscriber and views of channel youtube",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "description": "id of youtube channel",
            "required": true,
            "schema": {
              "type": "string",
              "example": "UCoIiiHof6BJ85PLuLkuxuhw"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "200 response",
            "examples": {
              "application/json": {
                "status": "string",
                "code": 200,
                "message": "string",
                "data": {
                  "views": 0,
                  "videos": 0
                }
              }
            }
          }
        }
      }
    }
  }
}
